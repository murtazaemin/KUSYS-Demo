
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<!--Page header-->
<div class="page-header">
    <div class="page-leftheader">
        <h4 class="page-title mb-0 text-primary"> Students' Course Matches </h4>
    </div>
    <div>
        <a class="btn btn-success" data-bs-toggle="modal" data-bs-target="#ModalStudentsCourseAdd"><i class="fa fa-plus-square"></i>Select Course</a>
    </div>
</div>
<!--End Page header-->
<!-- Row -->
<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-bordered text-nowrap" id="StudentsCourseTable">
                        <thead>
                            <tr>
                                <th scope="col">Student Name</th>
                                <th scope="col">Course Name</th>
                                <th scope="col">Course Code</th>
                                <th scope="col">Create Date</th>
                            </tr>
                        </thead>
                        <tbody id="SetStudentsCourseList">
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- End Row -->
<!-- The Modal -->
<div class="modal" id="ModalStudentsCourseAdd">
    <div class="modal-dialog">
        <div class="modal-content">

            <!-- Modal Header -->
            <div class="modal-header">
                <h4 class="modal-title">Select Course</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal">&times;</button>
            </div>

            <!-- Modal body -->
            <div class="modal-body">
                <form id="add_students_course_form" role="form">
                  
                   <div class="form-group">
                        <label class="control-label">Student Name</label>
                        <div>
                            <select class="form-select" name="studentName" id="studentName" > 
                               
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label">Course Name</label>
                        <div>
                            <select class="form-select" name="courseName" id="courseName" > 
                               
                            </select>
                        </div>
                    </div>


                    <div class="form-group container">
                        <div>
                            <button id="AddStudentsCourse" type="button" class="btn btn-success" data-bs-dismiss="modal">Add</button>

                            <button id="CancelStudentsCourse" type="button" class="btn btn-danger" data-bs-dismiss="modal">Cancel</button>
                        </div>
                    </div>
                </form>
            </div>

            <!-- Modal footer -->
            <div class="modal-footer">


            </div>

        </div>
    </div>
</div>

